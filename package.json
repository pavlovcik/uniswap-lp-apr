{
	"name": "uniswap-lp-bookmarklet",
	"version": "1.0.0",
	"main": "dist/index.js",
	"author": "アレクサンダー.eth <gpg@pavlovcik.com>",
	"license": "MIT",
	"scripts": {
		"build": "run-p _build format",
		"_build": "run-s _build:*",
		"_build:clean": "rimraf dist",
		"_build:main": "run-s compile _optimize _prepend",
		"_build:export": "cat dist/closured.js | pbcopy && echo 'Copied to clipboard'",
		"compile": "parcel build src/index.ts --dist-dir dist",
		"format": "run-s format:*",
		"format:prettier": "prettier --write .",
		"format:eslint": "eslint . --ext .ts,.tsx --cache --fix --plugin tsc --rule 'tsc/config: [2, {configFile: \"./tsconfig.json\"}]'",
		"_optimize": "google-closure-compiler dist/index.js --js_output_file dist/closured.js",
		"_prepend": "ts-node scripts/append-javascript.ts dist/closured.js",
		"_watch": "yarn nodemon --ignore dist --exec 'yarn build'"
	},
	"dependencies": {
		"@typescript-eslint/parser": "^5.10.2",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"google-closure-compiler": "^20220104.0.0",
		"nodemon": "^2.0.15",
		"npm-run-all": "^4.1.5",
		"parcel": "^2.2.1",
		"ts-node": "^10.4.0",
		"typescript": "^4.5.5",
		"web3": "^1.7.0"
	},
	"devDependencies": {
		"@ethersproject/providers": "^5.5.2",
		"@graphprotocol/graph-cli": "^0.26.0",
		"@typescript-eslint/eslint-plugin": "^5.10.2",
		"eslint": "^8.8.0",
		"eslint-plugin-tsc": "^2.0.0",
		"hardhat": "^2.8.3",
		"prettier": "^2.5.1"
	}
}
