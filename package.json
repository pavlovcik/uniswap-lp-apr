{
	"name": "uniswap-lp-bookmarklet",
	"version": "0.1.0",
	"author": "アレクサンダー.eth <gpg@pavlovcik.com>",
	"license": "MIT",
	"scripts": {
		"build": "run-p _build format",
		"format": "run-s _format:*",
		"serve": "static dist --port 8888",
		"_ci": "run-s _build:main",
		"_compile": "parcel build src/index.ts --dist-dir dist",
		"_format:prettier": "prettier --write .",
		"_format:eslint": "eslint . --ext .ts,.tsx --cache --fix --plugin tsc --rule 'tsc/config: [2, {configFile: \"./tsconfig.json\"}]'",
		"_format:typecheck": "tsc --noEmit",
		"_build": "run-s _build:*",
		"_build:clean": "rimraf dist .parcel-cache",
		"_build:main": "run-s _compile _optimize _prepend",
		"_optimize": "google-closure-compiler --compilation_level ADVANCED --js dist/index.js --create_source_map ./dist/closured-map --js_output_file dist/closured.js",
		"_prepend": "ts-node scripts/append-javascript.ts dist/closured.js",
		"_watch": "yarn nodemon --ignore dist --exec 'yarn build'"
	},
	"dependencies": {
		"google-closure-compiler": "^20220104.0.0",
		"node-static": "^0.7.11",
		"nodemon": "^2.0.15",
		"npm-run-all": "^4.1.5",
		"parcel": "^2.2.1",
		"ts-node": "^10.4.0",
		"typescript": "^4.5.5"
	},
	"devDependencies": {
		"@ethersproject/providers": "^5.5.2",
		"@graphprotocol/graph-cli": "^0.26.0",
		"@typescript-eslint/eslint-plugin": "^5.10.2",
		"@typescript-eslint/parser": "^5.10.2",
		"eslint": "^8.8.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-tsc": "^2.0.0",
		"hardhat": "^2.8.3",
		"prettier": "^2.5.1"
	}
}
